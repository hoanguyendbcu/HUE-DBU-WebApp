@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer ViewLocalizer
@using DBCU_WebApp.Areas.Identity.Pages.Account.Manage

@{ 
    ViewData["Title"] = "Thua Thien Hue DBU - " + ViewLocalizer["Photos"].Value;
    ViewData["ActivePagePar"] = ManageNavPages.Library;
    }

<!-- ======= Breadcrumbs ======= -->
<section class="breadcrumbs">
    <div class="container">
        <ol>
            <li><a asp-area="" asp-controller="Home" asp-action="Index">@ViewLocalizer["Home"]</a></li>
            <li>@ViewLocalizer["Photos"]</li>
        </ol>
    </div>
</section><!-- End Breadcrumbs -->
